12\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"13\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"14\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"15\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"16\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"17\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"18\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"19\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"20\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"21\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"22\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"23\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"24\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"25\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"26\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"27\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"28\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"29\")]),n(\"br\")])]),n(\"blockquote\",[n(\"p\",[s._v(\"其实严格的讲（在James Gosling等人编著的《The Java Language Specification》）静态内部类不是内部类，而是类似于C++的嵌套类的概念，外部类仅仅是静态内部类的一种命名空间的限定名形式而已。所以接口中的内部类通常都不叫内部类，因为接口中的内部成员都是隐式是静态的（即public static)。例如：Map.Entry。\")])])])]),s._v(\" \"),n(\"h3\",{attrs:{id:\"_7-3-3-非静态成员内部类\"}},[n(\"a\",{staticClass:\"header-anchor\",attrs:{href:\"#_7-3-3-非静态成员内部类\"}},[s._v(\"#\")]),s._v(\" 7.3.3 非静态成员内部类\")]),s._v(\" \"),n(\"ol\",[n(\"li\",[n(\"p\",[n(\"strong\",[s._v(\"语法格式：\")])]),s._v(\" \"),n(\"div\",{staticClass:\"language-java line-numbers-mode\"},[n(\"pre\",{pre:!0,attrs:{class:\"language-java\"}},[n(\"code\",[s._v(\"【修饰符】 \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"class\")]),s._v(\" 外部类\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),s._v(\"\\n    【修饰符】 \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"class\")]),s._v(\" 内部类\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),s._v(\"\\n    \"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"}\")]),s._v(\"\\n\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"}\")]),s._v(\"\\n\")])]),s._v(\" \"),n(\"div\",{staticClass:\"line-numbers-wrapper\"},[n(\"span\",{staticClass:\"line-number\"},[s._v(\"1\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"2\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"3\")]),n(\"br\"),n(\"span\",{staticClass:\"line-number\"},[s._v(\"4\")]),n(\"br\")])])]),s._v(\" \"),n(\"li\",[n(\"p\",[n(\"strong\",[s._v(\"非静态内部类的特点：\")])]),s._v(\" \"),n(\"ul\",[n(\"li\",[n(\"p\",[s._v(\"和其他类一样，它只是定义在外部类中的另一个完整的类结构，就是为了方便使用外部类的数据，与外部类的耦合度高。\")]),s._v(\" \"),n(\"ul\",[n(\"li\",[s._v(\"可以继承自己的想要继承的父类，实现自己想要实现的父接口们，和外部类的父类和父接口无关\")]),s._v(\" \"),n(\"li\",[s._v(\"可以在非静态内部类中声明属性、方法、构造器等结构，但是\"),n(\"strong\",[s._v(\"不允许声明静态成员\")]),s._v(\"，但是可以\"),n(\"strong\",[s._v(\"继承\")]),s._v(\"父类的静态成员，而且\"),n(\"strong\",[s._v(\"可以声明静态常量\")]),s._v(\"。\")]),s._v(\" \"),n(\"li\",[s._v(\"可以使用abstract修饰，因此它也可以被其他类继承\")]),s._v(\" \"),n(\"li\",[s._v(\"可以使用final修饰，表示不能被继承\")]),s._v(\" \"),n(\"li\",[s._v(\"编译后有自己的独立的字节码文件，只不过在内部类名前面冠以外部类名和$符号。\")])])]),s._v(\" \"),n(\"li\",[n(\"p\",[s._v(\"和外部类不同的是，它可以允许四种权限修饰符：public，protected，缺省，private\")]),s._v(\" \"),n(\"ul\",[n(\"li\",[s._v(\"外部类只允许public或缺省的\")])])]),s._v(\" \"),n(\"li\",[n(\"p\",[s._v(\"还可以在非静态内部类中使用外部类的\"),n(\"strong\",[s._v(\"所有成员\")]),s._v(\"，\"),n(\"strong\",[s._v(\"哪怕是私有的\")])])]),s._v(\" \"),n(\"li\",[n(\"p\",[s._v(\"在外部类的静态成员中不可以使用非静态内部类哦\")]),s._v(\" \"),n(\"ul\",[n(\"li\",[s._v(\"就如同静态方法中不能访问本类的非静态成员变量和非静态方法一样\")])])]),s._v(\" \"),n(\"li\",[n(\"p\",[s._v(\"在外部类的外面必须通过外部类的对象才能创建非静态内部类的对象\")]),s._v(\" \"),n(\"ul\",[n(\"li\",[s._v(\"因此在非静态内部类的方法中有两个this对象，一个是外部类的this对象，一个是内部类的this对象\")])])])])]),s._v(\" \"),n(\"li\",[n(\"p\",[n(\"strong\",[s._v(\"示例代码：\")])]),s._v(\" \"),n(\"div\",{staticClass:\"language-java line-numbers-mode\"},[n(\"pre\",{pre:!0,attrs:{class:\"language-java\"}},[n(\"code\",[n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"public\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"class\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token class-name\"}},[s._v(\"TestInner\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),s._v(\"\\n    \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"public\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"static\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"void\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token function\"}},[s._v(\"main\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"(\")]),n(\"span\",{pre:!0,attrs:{class:\"token class-name\"}},[s._v(\"String\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"[\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"]\")]),s._v(\" args\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\")\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"{\")]),s._v(\"\\n    \\t\"),n(\"span\",{pre:!0,attrs:{class:\"token class-name\"}},[s._v(\"Outer\")]),s._v(\" out \"),n(\"span\",{pre:!0,attrs:{class:\"token operator\"}},[s._v(\"=\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token keyword\"}},[s._v(\"new\")]),s._v(\" \"),n(\"span\",{pre:!0,attrs:{class:\"token class-name\"}},[s._v(\"Outer\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\"(\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\")\")]),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\";\")]),s._v(\"\\n    \\t\"),n(\"span\",{pre:!0,attrs:{class:\"token class-name\"}},[s._v(\"Outer\"),n(\"span\",{pre:!0,attrs:{class:\"token punctuation\"}},[s._v(\".\")]),s._v(\"Inner\")]),s._v(\" in\"),n(\"sp